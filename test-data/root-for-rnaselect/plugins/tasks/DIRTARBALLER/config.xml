<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<taskConfig xsi:noNamespaceSchemaLocation="../../../../../schemas/plugins.xsd"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <name>Tar compression utility for directories</name>
    <id>DIRTARBALLER</id>
    <help>Use GNU tar for creating TAR archives of directories</help>
    <version>1.0</version>
    <inputSchema>
        <fileSetRef maxOccurs="unbounded" minOccurs="1">
            <id>GENERIC_DIRECTORY</id>
            <versionAtMost>2.0</versionAtMost>
        </fileSetRef>
        <criteria/>
    </inputSchema>
    <outputSchema>
        <fileSetRef maxOccurs="1" minOccurs="1">
            <id>TAR</id>
            <version>1.0</version>
            <failIfNotProduced>true</failIfNotProduced>
        </fileSetRef>
    </outputSchema>
</taskConfig>